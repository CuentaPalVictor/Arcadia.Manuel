{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "PublicReadGetObject",
			"Effect": "Allow",
			"Principal": "*",
			"Action": "s3:GetObject",
			"Resource": "arn:aws:s3:::arcadia.mx/*"
		},
		{
			"Sid": "AllowAmplifyToListPrefix_d3efkgqcn8bmla_staging",
			"Effect": "Allow",
			"Principal": {
				"Service": "amplify.amazonaws.com"
			},
			"Action": "s3:ListBucket",
			"Resource": "arn:aws:s3:::arcadia.mx",
			"Condition": {
				"StringEquals": {
					"aws:SourceArn": "arn:aws:amplify:us-east-1:343075903042:apps/d3efkgqcn8bmla/branches/staging",
					"aws:SourceAccount": "343075903042"
				}
			}
		},
		{
			"Sid": "AllowAmplifyToReadPrefix_d3efkgqcn8bmla_staging",
			"Effect": "Allow",
			"Principal": {
				"Service": "amplify.amazonaws.com"
			},
			"Action": "s3:GetObject",
			"Resource": "arn:aws:s3:::arcadia.mx/*",
			"Condition": {
				"StringEquals": {
					"aws:SourceArn": "arn:aws:amplify:us-east-1:343075903042:apps/d3efkgqcn8bmla/branches/staging",
					"aws:SourceAccount": "343075903042"
				}
			}
		},
		{
			"Sid": "AllowCognitoUploads",
			"Effect": "Allow",
			"Principal": {
				"Federated": "cognito-identity.amazonaws.com"
			},
			"Action": [
				"s3:PutObject",
				"s3:GetObject",
				"s3:DeleteObject"
			],
			"Resource": "arn:aws:s3:::arcadia.mx/*"
		},
		{
			"Sid": "AllowCognitoListBucket",
			"Effect": "Allow",
			"Principal": {
				"Federated": "cognito-identity.amazonaws.com"
			},
			"Action": "s3:ListBucket",
			"Resource": "arn:aws:s3:::arcadia.mx"
		},
		{
			"Sid": "DenyInsecureConnections",
			"Effect": "Deny",
			"Principal": "*",
			"Action": "s3:*",
			"Resource": [
				"arn:aws:s3:::arcadia.mx",
				"arn:aws:s3:::arcadia.mx/*"
			],
			"Condition": {
				"Bool": {
					"aws:SecureTransport": "false"
				}
			}
		}
	]
}